<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.DeerTimer" parent="Theme.AppCompat.DayNight.NoActionBar" />

    <!-- UCrop 裁切界面主题 -->
    <style name="Theme.UCrop" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- 状态栏不覆盖内容 -->
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@android:color/black</item>
        <!-- 工具栏颜色 -->
        <item name="colorPrimary">#333333</item>
        <item name="colorPrimaryDark">#222222</item>
    </style>
</resources>